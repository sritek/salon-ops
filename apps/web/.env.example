# Web Frontend Environment Variables

NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1
NEXT_PUBLIC_APP_URL=http://localhost:3001

# Feature Flags
# Set to 'true' to enable real-time SSE features (requires Redis on backend)
NEXT_PUBLIC_ENABLE_REALTIME=false

# Module Feature Flags (Phase 1 pilot: inventory and memberships disabled)
NEXT_PUBLIC_ENABLE_INVENTORY=false
NEXT_PUBLIC_ENABLE_MEMBERSHIPS=false
