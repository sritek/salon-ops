{
    "version": 2,
    "installCommand": "cd ../.. && pnpm install",
    "buildCommand": "cd ../.. && pnpm --filter @salon-ops/shared build && pnpm --filter @salon-ops/api db:generate",
    "functions": {
        "api/index.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ]
}